<% if current_user == @prof.user %>
<h1>Listing profs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Pro</th>
    <th>Con</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profs.each do |prof| %>
  <tr>
    <td><%= prof.name %></td>
    <td><%= prof.email %></td>
    <td><%= prof.pro %></td>
    <td><%= prof.con %></td>
    <td><%= link_to 'Show', prof %></td>
    <td><%= link_to 'Edit', edit_prof_path(prof) %></td>
    <td><%= link_to 'Destroy', prof, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Prof', new_prof_path %>

<% end %>
