<!DOCTYPE html>
<html>
	<head>

  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
 	<%= analytics_init if Rails.env.production? %>

</head>
<body>

<%= render 'layouts/header' %>

<div class="container">
	<% flash.each do |name, msg| %>
	<%= content_tag(:div, msg, class: "alert alert-#{name}") %>
		<% end %>

	<%= yield %>
</div>
</body>
</html>
